{
  "mcpServers": {
    "websearch-docker": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-p", "8080:8080",
        "-e", "PORT=8080",
        "websearch-mcp:latest"
      ],
      "env": {
        "DOCKER_HOST": "unix:///var/run/docker.sock"
      }
    }
  }
}